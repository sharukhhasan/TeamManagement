<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sharukhhasan.teammanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.RegisterActivity" />
        <activity android:name=".activity.AdminListActivity" />
        <activity android:name=".activity.AdminSettingsActivity" />
        <activity android:name=".activity.ManagerListActivity" />
        <activity android:name=".activity.ManagerSettingsActivity" />
        <activity android:name=".activity.SelectListActivity" />
        <activity android:name=".activity.UserListActivity" />
        <activity android:name=".activity.UserSettingsActivity" />
        <activity android:name=".activity.UserTimeActivity"></activity>
    </application>

</manifest>